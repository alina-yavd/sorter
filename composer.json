{
  "name": "alina-yavd/sorter",
  "description": "PHP sorting library",
  "keywords": [
    "library"
  ],
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "AYSorter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AYSorter\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes --diff --ansi"
  }
}
